# API配置文件模板
# 使用说明：
# 1. 复制此文件并重命名为 api_config.yaml
# 2. 填入你自己的API密钥
# 3. api_config.yaml 不会被提交到git（已在.gitignore中）

openrouter:
  # 从 https://openrouter.ai/keys 获取你的API密钥
  api_key: "sk-or-v1-YOUR_API_KEY_HERE"
  
  # 使用 Qwen 2.5 72B
  default_model: "qwen/qwen-2.5-72b-instruct"
  
  # 模型参数
  temperature: 0.7      # 温度：0-1，越高越随机，越低越确定
  max_tokens: 1000      # 最大输出token数
  
  timeout: 30

output:
  output_dir: "analysis_results"
  use_timestamp: true
